{
  "private": true,
  "name": "dali",
  "version": "0.1.0",
  "main": "index.js",
  "repository": "https://github.com/yszk0123/dali.git",
  "author": "Yuji Suzuki <y0suzuki0123@gmail.com>",
  "license": "MIT",
  "scripts": {
    "build": "npm-run-all clean build:relay -p build:client build:server",
    "build:client": "webpack",
    "build:relay": "relay-compiler --src ./src/client/ --schema ./data/schema.graphql",
    "build:server": "babel src/server --out-dir dist/server",
    "clean": "rm -rf dist",
    "db:migrate": "sequelize db:migrate",
    "db:migrate:create": "sequelize migration:create",
    "db:migrate:rollback": "sequelize db:migrate:undo",
    "db:migrate:version": "sequelize version",
    "dev:build": "run-s build:relay build:client",
    "dev:repl": "node -r babel-register -r ./scripts/repl.js",
    "dev:serve": "node -r babel-register ./src/server/dev.js",
    "dev:start": "node -r babel-register ./src/server/index.js",
    "dev:watch": "run-p watch:relay watch:client",
    "format": "prettier './src/**/*.js' --write --single-quote --trailing-comma all",
    "lint": "eslint ./src ./data",
    "release": "NODE_ENV=production yarn build",
    "start": "node ./dist/server/index.js",
    "start:with-migrate": "yarn db:migrate && yarn start",
    "test": "jest",
    "test:ci": "yarn typecheck && jest --coverage && cat ./coverage/lcov.info | coveralls",
    "typecheck": "flow",
    "update:schema": "node -r babel-register ./scripts/updateSchema.js",
    "watch": "run-p watch:*",
    "watch:client": "yarn build:client -- --watch",
    "watch:relay": "yarn build:relay -- --watch",
    "watch:server": "yarn build:server -- --watch"
  },
  "dependencies": {
    "clipboard": "^1.7.1",
    "cookie-session": "^2.0.0-beta.2",
    "date-fns": "^1.28.5",
    "dotenv": "^4.0.0",
    "express": "^4.15.3",
    "express-graphql": "^0.6.6",
    "express-history-api-fallback": "^2.2.1",
    "express-http-proxy": "^1.0.4",
    "font-awesome": "^4.7.0",
    "graphiql": "^0.11.2",
    "graphql": "^0.10.1",
    "graphql-date": "^1.0.3",
    "graphql-errors": "^2.1.0",
    "graphql-relay": "^0.5.2",
    "graphql-sequelize": "^5.3.3",
    "jsonwebtoken": "^7.4.1",
    "lodash": "^4.17.4",
    "morgan": "^1.8.2",
    "normalize.css": "^7.0.0",
    "pg": "^6.4.0",
    "pg-hstore": "^2.3.2",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "react-modal": "^2.2.1",
    "react-relay": "^1.0.0",
    "react-router-dom": "^4.1.1",
    "sequelize": "^4.2.0",
    "sequelize-cli": "^2.7.0",
    "serve-favicon": "^2.4.3",
    "styled-components": "^2.0.0"
  },
  "devDependencies": {
    "autodll-webpack-plugin": "^0.2.1",
    "babel-cli": "^6.24.1",
    "babel-core": "^6.24.1",
    "babel-eslint": "^7.2.3",
    "babel-jest": "^20.0.3",
    "babel-loader": "^7.1.1",
    "babel-plugin-lodash": "^3.2.11",
    "babel-plugin-relay": "^1.0.1",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.5.1",
    "babel-preset-react": "^6.24.1",
    "babel-register": "^6.24.1",
    "babel-runtime": "^6.23.0",
    "babili-webpack-plugin": "^0.1.2",
    "css-loader": "^0.28.4",
    "enzyme": "^2.8.2",
    "eslint": "^4.1.1",
    "eslint-config-react-app": "^1.0.5",
    "eslint-plugin-flowtype": "^2.34.1",
    "eslint-plugin-import": "^2.6.1",
    "eslint-plugin-jsx-a11y": "^5.1.0",
    "eslint-plugin-react": "^7.0.1",
    "extract-text-webpack-plugin": "^2.1.2",
    "file-loader": "^0.11.2",
    "flow-bin": "^0.49.1",
    "html-webpack-harddisk-plugin": "^0.1.0",
    "html-webpack-plugin": "^2.29.0",
    "jest": "^20.0.4",
    "npm-run-all": "^4.0.2",
    "prettier": "^1.5.2",
    "react-test-renderer": "^15.5.4",
    "relay-compiler": "^1.0.0",
    "style-loader": "^0.18.2",
    "url-loader": "^0.5.9",
    "webpack": "^3.0.0",
    "webpack-dev-server": "^2.4.5"
  }
}
