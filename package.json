{
  "private": true,
  "name": "dali",
  "version": "0.1.0",
  "main": "index.js",
  "repository": "https://github.com/yszk0123/dali.git",
  "author": "Yuji Suzuki <y0suzuki0123@gmail.com>",
  "license": "MIT",
  "scripts": {
    "start": "babel-node ./server.js",
    "test": "jest",
    "test:ci": "yarn typecheck && jest --coverage && cat ./coverage/lcov.info | coveralls",
    "typecheck": "flow",
    "format": "prettier './src/**/*.js' --write --single-quote --trailing-comma all",
    "build": "run-s build:relay build:js",
    "build:js": "webpack",
    "build:relay": "relay-compiler --src ./src/ --schema ./data/schema.graphql",
    "watch": "relay-compiler --src ./src/ --schema ./data/schema.graphql --watch",
    "update:schema": "babel-node ./scripts/updateSchema.js",
    "lint": "eslint ./src ./data"
  },
  "dependencies": {
    "express": "^4.15.3",
    "express-graphql": "^0.6.6",
    "graphiql": "^0.10.2",
    "graphql": "^0.10.1",
    "graphql-relay": "^0.5.2",
    "normalize.css": "^7.0.0",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "react-relay": "^1.0.0",
    "styled-components": "^2.0.0"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.24.1",
    "babel-eslint": "^7.2.3",
    "babel-jest": "^20.0.3",
    "babel-loader": "^7.0.0",
    "babel-plugin-relay": "^1.0.1",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.5.1",
    "babel-preset-react": "^6.24.1",
    "babel-runtime": "^6.23.0",
    "babili-webpack-plugin": "^0.1.1",
    "css-loader": "^0.28.4",
    "enzyme": "^2.8.2",
    "eslint": "^3.19.0",
    "eslint-config-react-app": "^1.0.4",
    "eslint-plugin-flowtype": "^2.34.0",
    "eslint-plugin-import": "^2.3.0",
    "eslint-plugin-jsx-a11y": "^5.0.3",
    "eslint-plugin-react": "^7.0.1",
    "extract-text-webpack-plugin": "^2.1.2",
    "flow-bin": "^0.48.0",
    "html-webpack-harddisk-plugin": "^0.1.0",
    "html-webpack-plugin": "^2.28.0",
    "jest": "^20.0.4",
    "npm-run-all": "^4.0.2",
    "prettier": "^1.4.4",
    "react-test-renderer": "^15.5.4",
    "relay-compiler": "^1.0.0",
    "style-loader": "^0.18.2",
    "webpack": "^2.6.1",
    "webpack-dev-server": "^2.4.5"
  }
}
