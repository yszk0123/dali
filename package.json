  "scripts": {
    "build": "npm-run-all clean -p build:*",
    "build:client": "webpack",
    "build:graphql": "cpx 'src/server/graphql/**/*.graphql' dist/server/graphql",
    "build:server": "tsc -p tsconfig.server.json",
    "clean": "rm -rf dist",
    "db:migrate": "sequelize db:migrate",
    "db:migrate:create": "sequelize migration:create",
    "db:migrate:rollback": "sequelize db:migrate:undo",
    "db:migrate:version": "sequelize version",
    "db:seed": "echo \"To Be Implemented\" && exit 1",
    "db:seed:reset": "node -r babel-register ./scripts/generateFakeData.js",
    "format": "prettier './src/**/*.{js,jsx,ts,tsx}' --write --single-quote --trailing-comma all",
    "graphql:generate": "npm-run-all graphql:generate:schema graphql:generate:json graphql:generate:type",
    "graphql:generate:json": "apollo-codegen introspect-schema data/schema.graphql --output data/schema.json",
    "graphql:generate:schema": "node -r babel-register ./scripts/generateSchema.js",
    "graphql:generate:type": "apollo-codegen generate 'src/client/graphql/**/*.graphql' --schema data/schema.json --target typescript --output data/schema.ts",
    "lint": "eslint ./src ./data --ext .ts --ext tsx --fix",
    "release": "NODE_ENV=production yarn build",
    "repl": "node -r babel-register -r ./scripts/repl.js",
    "start": "node ./dist/server/index.js",
    "start:with-migrate": "yarn db:migrate && yarn start",
    "test": "NODE_ENV=test jest --config jestrc.json",
    "test:ci": "jest --config jestrc.json --coverage && cat ./coverage/lcov.info | coveralls",
    "watch": "run-p watch:client watch:server watch:graphql watch:graphql-type",
    "watch:client": "yarn build:client -- --progress --watch",
    "watch:graphql-type": "nodemon --exec 'yarn graphql:generate:type' --watch ./src/client/graphql --ext graphql --delay 1",
    "watch:graphql": "yarn build:graphql -- --watch",
    "watch:server": "yarn build:server -- --watch",
    "watch:start": "nodemon --watch ./dist/server --watch ./dist/shared --delay 2 ./dist/server/index.js",
    "watch:test": "NODE_ENV=test jest --config jestrc.json -o"
  },
